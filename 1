import numpy as np


def randomizing(x):
    return list(np.random.randint(10, 50, x))


def summing(list1):
    sum_elements = 0
    for i in list1:
        sum_elements = sum_elements + i
    return sum_elements


while True:
    user_input = int(input('Enter an integer between 5 and 10: '))
    if (user_input >= 5) and (user_input <= 15):
        break
    else:
        print('Stay in range')

randArray = randomizing(user_input)
sumArray = summing(randArray)

print(randArray)
print(sumArray)
